{
  "affected": { "defaultBase": "main" },
  "npmScope": "decipad",
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx-cloud",
      "options": {
        "cacheableOperations": ["lint", "test", "depcheck"],
        "runtimeCacheInputs": [
          "node -v",
          "echo DEPLOY_NAME=$DEPLOY_NAME",
          "echo NODE_OPTIONS=$NODE_OPTIONS",
          "echo GIT_COMMIT_HASH=$GIT_COMMIT_HASH",
          "echo SENTRY_DSN=$SENTRY_DSN",
          "echo SENTRY_ORG=$SENTRY_ORG",
          "echo SENTRY_PROJECT=$SENTRY_PROJECT",
          "echo SENTRY_AUTH_TOKEN=$SENTRY_AUTH_TOKEN",
          "echo SENTRY_ENVIRONMENT=$SENTRY_ENVIRONMENT",
          "echo NEXT_PUBLIC_ANALYTICS_WRITE_KEY=$NEXT_PUBLIC_ANALYTICS_WRITE_KEY",
          "echo NEXT_PUBLIC_ANALYTICS_WRITE_KEY=$NEXT_PUBLIC_ANALYTICS_WRITE_KEY",
          "echo NEXT_PUBLIC_HOTJAR_SITE_ID=$NEXT_PUBLIC_HOTJAR_SITE_ID",
          "echo NEXTAUTH_URL=$NEXTAUTH_URL",
          "echo GIT_COMMIT_HASH=$GIT_COMMIT_HASH",
          "echo GITHUB_CLIENT_ID=$GITHUB_CLIENT_ID",
          "echo GITHUB_CLIENT_SECRET=$GITHUB_CLIENT_SECRET",
          "echo JWT_SECRET=$JWT_SECRET",
          "echo MAILERSEND_API_KEY=$MAILERSEND_API_KEY",
          "echo DECI_INVITE_EXPIRATION_SECONDS=$DECI_INVITE_EXPIRATION_SECONDS",
          "echo DECI_KEY_VALIDATION_EXPIRATION_SECONDS=$DECI_KEY_VALIDATION_EXPIRATION_SECONDS",
          "echo DECI_FROM_EMAIL_ADDRESS=$DECI_FROM_EMAIL_ADDRESS",
          "echo DECI_APP_URL_BASE=$DECI_APP_URL_BASE",
          "echo DECI_S3_ENDPOINT=$DECI_S3_ENDPOINT",
          "echo DECI_S3_ACCESS_KEY_ID=$DECI_S3_ACCESS_KEY_ID",
          "echo DECI_S3_SECRET_ACCESS_KEY=$DECI_S3_SECRET_ACCESS_KEY",
          "echo DECI_S3_PADS_BUCKET=$DECI_S3_PADS_BUCKET",
          "echo DECI_S3_PAD_BACKUPS_BUCKET=$DECI_S3_PAD_BACKUPS_BUCKET",
          "echo DECI_S3_ATTACHMENTS_BUCKET=$DECI_S3_ATTACHMENTS_BUCKET",
          "echo DECI_GOOGLESHEETS_API_KEY=$DECI_GOOGLESHEETS_API_KEY",
          "echo DECI_GOOGLESHEETS_CLIENT_ID=$DECI_GOOGLESHEETS_CLIENT_ID",
          "echo DECI_GOOGLESHEETS_CLIENT_SECRET=$DECI_GOOGLESHEETS_CLIENT_SECRET",
          "echo DECI_MAX_CREDITS_FREE",
          "echo DECI_MAX_CREDITS_PRO",
          "echo DECI_MAX_QUERIES_FREE",
          "echo DECI_MAX_QUERIES_PRO",
          "echo DISCORD_PUBLIC_KEY=$DISCORD_PUBLIC_KEY",
          "echo DISCORD_APP_ID=$DISCORD_APP_ID",
          "echo ARC_STATIC_SPA=$ARC_STATIC_SPA",
          "echo OPENAI_API_KEY=$OPENAI_API_KEY",
          "echo GIPHY_API_KEY=$GIPHY_API_KEY",
          "echo UNSPLASH_API_KEY=$UNSPLASH_API_KEY",
          "echo REPLICATE_API_KEY=$REPLICATE_API_KEY",
          "echo STRIPE_API_KEY=$STRIPE_API_KEY",
          "echo STRIPE_SECRET_KEY=$STRIPE_SECRET_KEY",
          "echo STRIPE_EXTRA_CREDITS_PRODUCT_ID=$STRIPE_EXTRA_CREDITS_PRODUCT_ID",
          "echo STRIPE_WEBHOOK_SECRET=$STRIPE_WEBHOOK_SECRET",
          "echo WORKSPACE_FREE_PLAN=$WORKSPACE_FREE_PLAN",
          "echo WORKSPACE_PRO_PLAN=$WORKSPACE_PRO_PLAN"
        ],
        "parallel": 12,
        "accessToken": "ODYzMjhhZjAtMTNiZS00YWVjLWE3M2ItN2VmYjZkNTc3MTFlfHJlYWQtd3JpdGU="
      }
    }
  },
  "defaultProject": "frontend",
  "targetDefaults": {
    "lint": { "inputs": ["default"] },
    "build": { "inputs": ["production", "^production"] },
    "test": {
      "inputs": ["default", "^production", "{workspaceRoot}/jest.preset.js"]
    },
    "build-storybook": {
      "inputs": [
        "default",
        "^production",
        "{projectRoot}/.storybook/**/*",
        "{projectRoot}/tsconfig.json"
      ]
    }
  },
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "namedInputs": {
    "default": ["{projectRoot}/**/*", "sharedGlobals", "projectSpecificFiles"],
    "sharedGlobals": [
      "{workspaceRoot}/workspace.json",
      "{workspaceRoot}/tsconfig.json",
      "{workspaceRoot}/tslint.json",
      "{workspaceRoot}/libs/eslint-config-decipad/index.js",
      "{workspaceRoot}/libs/eslint-config-decipad/react.js",
      "{workspaceRoot}/nx.json"
    ],
    "production": [
      "default",
      "!{projectRoot}/**/?(*.)+(spec|test).[jt]s?(x)?(.snap)",
      "!{projectRoot}/tsconfig.spec.json",
      "!{projectRoot}/jest.config.[jt]s",
      "!{projectRoot}/**/*.stories.@(js|jsx|ts|tsx|mdx)",
      "!{projectRoot}/.storybook/**/*"
    ],
    "projectSpecificFiles": []
  }
}
