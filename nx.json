{
  "implicitDependencies": {
    "workspace.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tsconfig.json": "*",
    "tslint.json": "*",
    ".eslintrc.json": "*",
    "nx.json": "*"
  },
  "affected": {
    "defaultBase": "main"
  },
  "npmScope": "decipad",
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "lint",
          "test",
          "e2e",
          "build-storybook"
        ]
      }
    }
  },
  "projects": {
    "backend": {
      "tags": []
    },
    "client": {
      "tags": [],
      "implicitDependencies": ["backend"]
    },
    "client-e2e": {
      "tags": [],
      "implicitDependencies": ["client"]
    },
    "editor": {
      "tags": []
    },
    "kafka-lite": {
      "tags": []
    },
    "language": {
      "tags": []
    },
    "queries": {
      "tags": []
    },
    "runtime": {
      "tags": []
    },
    "ui": {
      "tags": []
    }
  }
}
