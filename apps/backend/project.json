{
  "name": "backend",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/backend/src",
  "projectType": "application",
  "prefix": "backend",
  "targets": {
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["./scripts/sandbox.sh"],
        "cwd": "apps/backend"
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "yarn tsc -b apps/backend"
      }
    }
  },
  "tags": [],
  "implicitDependencies": [
    "nodelambda",
    "lambdas",
    "utils",
    "interfaces",
    "backendtypes",
    "graphql-client"
  ]
}
