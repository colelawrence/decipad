{
  "root": "apps/docs",
  "sourceRoot": "apps/docs/src",
  "projectType": "application",
  "prefix": "docs",
  "targets": {
    "serve": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": ["../../node_modules/.bin/docusaurus start"],
        "cwd": "apps/docs"
      }
    },
    "build": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": ["../../node_modules/.bin/docusaurus build"],
        "cwd": "apps/docs"
      }
    },
    "test": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "node apps/docs/snapshots.mjs",
        "cwd": "."
      }
    }
  }
}
