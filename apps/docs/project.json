{
  "name": "docs",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/docs/src",
  "projectType": "application",
  "prefix": "docs",
  "implicitDependencies": ["language", "computer"],
  "targets": {
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["npx docusaurus start"],
        "cwd": "apps/docs"
      }
    },
    "serveproduction": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["yarn serve --build"],
        "cwd": "apps/docs"
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["npx docusaurus build"],
        "cwd": "apps/docs"
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "yarn tsc -b apps/docs"
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "options": {
        "lintFilePatterns": ["apps/docs/**/*.{js,ts}"],
        "maxWarnings": 0
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node snapshots.mjs",
        "cwd": "apps/docs"
      }
    }
  },
  "scripts": {
    "production": "yarn serve --build"
  },
  "tags": []
}
