    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.23.2",
    "eslint-plugin-react-hooks": "4.2.0",
    "get-port": "^5.1.1",
    "husky": "^6.0.0",
    "isomorphic-fetch": "^3.0.0",
    "jest": "26.6.3",
    "jest-fetch-mock": "^3.0.3",
    "lint-staged": "^11.0.0",
    "lorem-ipsum": "^2.0.3",
    "mock-socket": "^9.0.3",
    "prettier": "2.3.0",
    "realpath": "^1.1.0",
