{
  "name": "decipad",
  "version": "0.0.0",
  "license": "UNLICENSED",
  "scripts": {
    "nx": "nx",
    "start": "nx serve",
    "build": "nx build",
    "test": "nx run-many --target=test --all",
    "test:coverage": "nx run-many --target=test --all --code-coverage",
    "lint": "nx workspace-lint && nx run-many --target=lint --all",
    "typecheck": "tsc --incremental --project ./tsconfig.json",
    "e2e": "nx run client-e2e:e2e",
    "repl": "ts-node --project ./libs/language/tsconfig.json -O '{\"module\": \"commonjs\"}' -T libs/language/src/repl.ts",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "build:grammar": "nearleyc libs/language/src/grammar/deci-language-grammar.ne -o libs/language/src/grammar/deci-language-grammar-generated.js && prettier --write libs/language/src/grammar/deci-language-grammar-generated.js ",
    "postinstall": "cd apps/backend && npm install",
    "prepare": "husky install",
    "build:frontend": "nx export client",
    "build:backend:dev": "esbuild apps/backend/lib/architect/http/*/index.ts apps/backend/lib/architect/queues/*/index.ts apps/backend/lib/architect/ws/*/index.ts --bundle --outdir=apps/backend/src --target=node14 --platform=node --format=cjs --external:aws-sdk --legal-comments=none --sourcemap --keep-names",
    "build:backend": "npm run build:backend:dev -- --minify",
    "build:backend:watch": "npm run build:backend -- --watch",
    "serve:all": "nx run-many --target=serve --projects=client,backend --parallel",
    "deploy": "nx export client && cd apps/backend && ./node_modules/.bin/arc deploy --no-hydrate",
    "deploy:private": "./scripts/deploy-private.sh",
    "deploy:private:client": "./scripts/deploy-private-client.sh"
  },
  "private": true,
