    "@storybook/react": "^6.2.9",
    "@svgr/webpack": "^5.5.0",
    "@testing-library/react": "11.2.6",
    "@types/jest": "26.0.23",
    "@types/lodash.debounce": "^4.0.6",
    "@types/nearley": "^2.11.1",
    "@types/node": "15.0.3",
    "@types/react": "17.0.5",
    "@types/react-dom": "17.0.4",
