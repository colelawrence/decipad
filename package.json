    "lodash.debounce": "^4.0.8",
    "lodash.uniqby": "^4.7.0",
    "nanoid": "^3.1.22",
    "nearley": "^2.20.1",
    "next": "^10.2.0",
    "next-auth": "^3.21.1",
    "prismjs": "^1.23.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
