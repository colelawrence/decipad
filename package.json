  "private": true,
  "name": "decipad",
  "version": "0.2.3-alpha",
  "license": "UNLICENSED",
  "workspaces": [
    "apps/backend",
    "apps/docs",
    "apps/frontend",
    "libs/ui"
  ],
  "engines": {
    "node": "=16"
  },
  "scripts": {
    "start": "yarn serve:all",
    "serve:all": "run-p serve:backend serve:frontend",
    "serve:all:e2e": "DECI_E2E=1 run-p serve:backend serve:frontend",
    "serve:backend": "nx serve backend",
    "serve:frontend": "nx serve frontend",
    "serve:storybook": "nx run ui:storybook",
    "serve:docs": "nx run docs:serve",
    "build": "nx build",
    "build:grammar": "cd libs/language/src/grammar/nearley; nearleyc deci-language-grammar.ne | prettier --stdin-filepath grammar.js > deci-language-grammar-generated.js",
    "build:grammar:watch": "yarn build:grammar; chokidar 'libs/language/src/grammar/**/*.ne' -c 'yarn build:grammar'",
    "build:frontend": "nx build frontend",
    "build:storybook": "nx run ui:build-storybook",
    "build:backend:dev": "node scripts/build.mjs",
    "build:backend": "MINIFY=true yarn build:backend:dev",
    "build:backend:watch": "WATCH=true yarn build:backend",
    "build:graphql": "graphql-codegen --config codegen.yml",
    "build:docs": "nx build docs",
    "lint": "nx workspace-lint && nx run-many --target=lint --all",
    "typecheck": "tsc -b libs/*/tsconfig.json apps/*/tsconfig.json",
    "typecheck:watch": "yarn typecheck -w",
    "test": "yarn build:backend && TZ=America/Los_Angeles nx run-many --target=test --all",
    "test:coverage": "yarn build:backend && TZ=America/Los_Angeles nx run-many --target=test --all --code-coverage",
    "e2e": "./scripts/e2e.sh",
    "deploy": "./scripts/deploy.sh",
    "deploy:private": "./scripts/deploy-private.sh",
    "deploy:private:client": "./scripts/deploy-private-client.sh",
    "deploy:pr": "./scripts/deploy-pr.sh",
    "deploy:alpha": "./scripts/deploy-alpha.sh",
    "deploy:discord-customer-support-bot": "./scripts/deploy-discord-customer-support-bot-commands.mjs",
    "repl": "babel-node --inspect --extensions=.js,.jsx,.ts,.tsx libs/repl/src/repl.ts",
    "nx": "nx",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "postinstall": "patch-package && yarn clean",
    "clean": "rm -rf dist apps/backend/public libs/*/*.tsbuildinfo apps/*/*.tsbuildinfo libs/*/*/.cache apps/*/*/.cache",
    "clean:deps": "rm -rf node_modules apps/backend/node_modules",
    "clean:all": "yarn clean && yarn clean:deps"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && nx affected:test"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,yml,yaml,md}": [
      "prettier --write"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --cache --fix --max-warnings 0"
    ]
  },
