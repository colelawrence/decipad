  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && jest --config=./jest.config.js --onlyChanged --maxWorkers=1 --testTimeout=10000 && yarn typecheck"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,yml,yaml}": [
      "prettier --write"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --cache --fix",
      "jest --passWithNoTests"
    ]
  },
  "dependencies": {
