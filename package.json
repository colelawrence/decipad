{
  "name": "decipad",
  "version": "0.0.0",
  "license": "UNLICENSED",
  "scripts": {
    "nx": "nx",
    "start": "nx serve",
    "build": "nx build",
    "test": "nx run-many --target=test --all",
    "test:coverage": "nx run-many --target=test --all --code-coverage",
    "lint": "nx workspace-lint && nx run-many --target=lint --all",
    "typecheck": "tsc --incremental --project ./tsconfig.json",
    "e2e": "nx run client-e2e:e2e",
    "repl": "ts-node --project ./libs/language/tsconfig.json -O '{\"module\": \"commonjs\"}' -T libs/language/src/repl.ts",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "build:grammar": "nearleyc libs/language/src/grammar/deci-language-grammar.ne -o libs/language/src/grammar/deci-language-grammar-generated.js && prettier --write libs/language/src/grammar/deci-language-grammar-generated.js ",
    "postinstall": "cd apps/backend && npm install",
    "deploy": "nx export client && cd apps/backend && ./node_modules/.bin/arc deploy",
    "deploy:pr": "nx export client && cd apps/backend && ./node_modules/.bin/arc deploy --name ",
    "undeploy:pr": "cd apps/backend && ./node_modules/.bin/arc destroy ",
    "prepare": "husky install"
  },
  "private": true,
  "devDependencies": {
    "@architect/sandbox": "^3.4.3",
    "@babel/core": "7.14.2",
    "@babel/plugin-proposal-decorators": "^7.14.2",
    "@babel/plugin-transform-runtime": "^7.14.2",
    "@babel/preset-env": "7.14.2",
    "@babel/preset-react": "7.13.13",
    "@babel/preset-typescript": "7.13.0",
    "@emotion/babel-preset-css-prop": "11.2.0",
    "@nrwl/cli": "12.2.0",
    "@nrwl/cypress": "12.2.0",
    "@nrwl/eslint-plugin-nx": "12.2.0",
    "@nrwl/jest": "12.2.0",
    "@nrwl/linter": "12.2.0",
    "@nrwl/next": "^12.2.0",
    "@nrwl/node": "12.2.0",
