{
  "index_name": "docs.decipad.com",
  "start_urls": [
    {
      "url": "https://dev.decipad.com/docs/docs/language",
      "selectors_key": "language",
      "tags": ["language"]
    }
  ],
  "selectors": {
    "default": {
      "lvl0": {
        "selector": "#navmenu .active",
        "global": true,
        "default_value": "Language"
      },
      "lvl1": "article h1",
      "lvl2": "article h2",
      "lvl3": "article h3",
      "lvl4": "article h4",
      "lvl5": "article h5",
      "text": "article p, article li",
      "lang": {
        "selector": "/html/@lang",
        "type": "xpath",
        "global": true,
        "default_value": "en-US"
      }
    },
    "language": {
      "lvl0": "article h1",
      "lvl1": "article h2",
      "lvl2": "article h3",
      "lvl3": "article h4",
      "lvl4": "article h5",
      "lvl5": "article td:first-child",
      "text": "article p, article li, article td:nth-child(2)",
      "lang": {
        "selector": "/html/@lang",
        "type": "xpath",
        "global": true,
        "default_value": "en-US"
      }
    }
  }
}
