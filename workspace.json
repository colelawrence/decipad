{
  "version": 2,
  "cli": {
    "defaultCollection": "@nrwl/next"
  },
  "defaultProject": "client",
  "generators": {
    "@nrwl/react": {
      "application": {
        "babel": true
      }
    },
    "@nrwl/next": {
      "application": {
        "style": "@emotion/styled",
        "linter": "eslint"
      }
    }
  },
  "projects": {
    "backend": {
      "root": "apps/backend",
      "sourceRoot": "apps/backend/src",
      "projectType": "application",
      "prefix": "backend",
      "targets": {
        "serve": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": ["./scripts/sandbox.sh"],
            "cwd": "apps/backend"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/backend/tests/**/*.ts"],
            "maxWarnings": 0
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/backend"],
          "options": {
            "jestConfig": "apps/backend/jest.config.js",
            "codeCoverage": false
          }
        },
        "typecheck": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "command": "yarn tsc -b apps/backend"
          }
        }
      }
    },
    "backendtypes": {
      "root": "libs/backendtypes",
      "sourceRoot": "libs/backendtypes/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/backendtypes/src/**/*.ts"],
            "maxWarnings": 0
          }
        },
        "typecheck": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "command": "yarn tsc -b libs/backendtypes"
          }
        }
      }
    },
    "client": {
      "root": "apps/client",
      "sourceRoot": "apps/client",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/next:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "root": "apps/client",
            "outputPath": "dist/apps/client"
          },
          "configurations": {
            "production": {}
          }
        },
        "serve": {
          "executor": "@nrwl/next:server",
          "options": {
            "buildTarget": "client:build",
            "dev": true,
            "proxyConfig": "apps/client/proxy.conf.json"
          },
          "configurations": {
            "production": {
              "buildTarget": "client:build:production",
              "dev": false
            }
          }
        },
        "export": {
          "executor": "@nrwl/next:export",
          "options": {
            "buildTarget": "client:build:production"
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/client"],
          "options": {
            "jestConfig": "apps/client/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/client/**/*.{ts,tsx,js,jsx}"],
            "maxWarnings": 0
          }
        },
        "typecheck": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "command": "yarn tsc -b apps/client"
          }
        }
      }
    },
