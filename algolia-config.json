{
  "index_name": "docs.decipad.com",
  "start_urls": [
    {
      "url": "https://dev.decipad.com/docs/docs/language",
      "selectors_key": "language",
      "tags": ["language"]
    }
  ],
  "selectors": {
    "default": {
      "lvl0": {
        "selector": "#navmenu .active",
        "global": true,
        "default_value": "Language"
      },
      "lvl1": "#page-doc h1",
      "lvl2": "#page-doc h2",
      "lvl3": "#page-doc h3",
      "lvl4": "#page-doc h4",
      "lvl5": "#page-doc h5",
      "text": "#page-doc p, #page-doc li",
      "lang": {
        "selector": "/html/@lang",
        "type": "xpath",
        "global": true,
        "default_value": "en-US"
      }
    },
    "language": {
      "lvl0": "#api-doc h1",
      "lvl1": "#api-doc h2",
      "lvl2": "#api-doc h3",
      "lvl3": "#api-doc h4",
      "lvl4": "#api-doc h5",
      "lvl5": "#api-doc td:first-child",
      "text": "#api-doc p, #api-doc li, #api-doc td:nth-child(2)",
      "lang": {
        "selector": "/html/@lang",
        "type": "xpath",
        "global": true,
        "default_value": "en-US"
      }
    }
  }
}
