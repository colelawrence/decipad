{
  "name": "parse",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/languagparse/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nx/linter:eslint",
      "options": {
        "lintFilePatterns": ["libs/parse/**/*.ts", "libs/parse/**/*.tsx"],
        "maxWarnings": 0
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "outputs": ["{workspaceRoot}/coverage/libs/parse"],
      "options": {
        "command": "npx vitest --run --config ../../vitest.base.config.ts",
        "cwd": "libs/parse"
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "yarn tsc -b libs/parse"
      }
    }
  },
  "tags": []
}
