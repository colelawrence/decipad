{
  "name": "live-connect",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/live-connect/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nx/linter:eslint",
      "options": {
        "lintFilePatterns": ["libs/live-connect/**/*.{ts,tsx,jsx}"],
        "maxWarnings": 0
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "outputs": ["{workspaceRoot}/coverage/libs/live-connect"],
      "options": {
        "command": "npx vitest --run",
        "cwd": "libs/live-connect"
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "yarn tsc -b libs/live-connect"
      }
    },
    "depcheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "../../node_modules/.bin/madge --circular src",
        "cwd": "libs/live-connect"
      }
    }
  },
  "tags": []
}
