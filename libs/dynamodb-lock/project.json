{
  "name": "dynamodb-lock",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/dynamodb-lock/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nx/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/dynamodb-lock/**/*.ts",
          "libs/dynamodb-lock/**/*.tsx"
        ],
        "maxWarnings": 0
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "outputs": ["{workspaceRoot}/coverage/libs/dynamodb-lock"],
      "options": {
        "command": "npx vitest --run",
        "cwd": "libs/dynamodb-lock"
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "yarn tsc -b libs/dynamodb-lock"
      }
    },
    "depcheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "../../node_modules/.bin/depcruise --config ../../.dependency-cruiser.js src",
        "cwd": "libs/dynamodb-lock"
      }
    }
  },
  "tags": []
}
