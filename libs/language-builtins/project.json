{
  "name": "language-builtins",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/language-builtins/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nx/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/language-builtins/**/*.ts",
          "libs/language-builtins/**/*.tsx"
        ],
        "maxWarnings": 0
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "outputs": ["{workspaceRoot}/coverage/libs/language-builtins"],
      "options": {
        "command": "npx vitest --run && npx vitest bench --outputJson benchmark.json",
        "cwd": "libs/language-builtins"
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "yarn tsc -b libs/language-builtins"
      }
    },
    "depcheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "../../node_modules/.bin/depcruise --config ../../.dependency-cruiser.js src",
        "cwd": "libs/language-builtins"
      }
    }
  },
  "tags": []
}
