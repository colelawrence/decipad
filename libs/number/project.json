{
  "name": "number",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/number/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nx/linter:eslint",
      "options": {
        "lintFilePatterns": ["libs/number/**/*.ts", "libs/number/**/*.tsx"],
        "maxWarnings": 0
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "yarn tsc -b libs/number"
      }
    },
    "depcheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "../../node_modules/.bin/depcruise --config ../../.dependency-cruiser.js src",
        "cwd": "libs/number"
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/libs/number"],
      "options": {
        "jestConfig": "libs/number/jest.config.js"
      }
    }
  }
}
