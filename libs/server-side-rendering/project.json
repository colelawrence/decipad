{
  "name": "server-side-rendering",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/server-side-rendering/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["libs/server-side-rendering/**/*.{ts,tsx,js,jsx}"],
        "maxWarnings": 0
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "yarn tsc -b libs/server-side-rendering"
      }
    },
    "depcheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "../../node_modules/.bin/madge --circular src",
        "cwd": "libs/server-side-rendering"
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "(cd ../../apps/frontend && node build-live-connect-worker.mjs) && ./scripts/build-ssr-lambdas.mjs",
        "cwd": "libs/server-side-rendering"
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "../../node_modules/.bin/craco start",
        "cwd": "libs/server-side-rendering"
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/libs/server-side-rendering"],
      "options": {
        "jestConfig": "libs/server-side-rendering/jest.config.js"
      }
    }
  },
  "tags": []
}
