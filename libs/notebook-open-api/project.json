{
  "name": "notebook-open-api",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/notebook-open-api/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nx/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/notebook-open-api/src/**/*.ts",
          "libs/notebook-open-api/src/**/*.tsx"
        ],
        "maxWarnings": 0
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "outputs": ["{workspaceRoot}/coverage/libs/notebook-open-api"],
      "options": {
        "command": "npx vitest --run",
        "cwd": "libs/notebook-open-api"
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "yarn tsc -b libs/notebook-open-api"
      }
    },
    "depcheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "../../node_modules/.bin/madge --circular src",
        "cwd": "libs/notebook-open-api"
      }
    }
  },
  "tags": []
}
