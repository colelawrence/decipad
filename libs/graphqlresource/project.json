{
  "name": "graphqlresource",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/graphqlresource/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nx/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/graphqlserver/src/**/*.ts",
          "libs/graphqlserver/src/**/*.tsx"
        ]
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "yarn tsc -b libs/graphqlresource"
      }
    },
    "depcheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "../../node_modules/.bin/depcruise --config ../../.dependency-cruiser.js src",
        "cwd": "libs/graphqlresource"
      }
    }
  },
  "tags": []
}
