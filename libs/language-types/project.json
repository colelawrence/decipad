{
  "name": "language-types",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/language-types/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nx/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/language-types/**/*.ts",
          "libs/language-types/**/*.tsx"
        ],
        "maxWarnings": 0
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "yarn tsc -b libs/language-types"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "outputs": ["{workspaceRoot}/coverage/libs/language-types"],
      "options": {
        "command": "npx vitest --run --config ../../vitest.base.config.ts",
        "cwd": "libs/language-types"
      }
    }
  },
  "tags": []
}
