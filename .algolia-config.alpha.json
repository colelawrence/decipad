{
  "index_name": "docs.alpha.decipad.com",
  "start_urls": [
    {
      "url": "https://app.decipad.com/docs/"
    }
  ],
  "sitemap_urls": ["https://app.decipad.com/docs/sitemap.xml"],
  "stop_urls": ["https://app.decipad.com/docs/releases/.*"],
  "selectors": {
    "default": {
      "lvl0": {
        "selector": "nav.menu a[aria-expanded=true]",
        "global": true,
        "default_value": "Quick Start"
      },
      "lvl1": "article h1",
      "lvl2": "article h2",
      "lvl3": "article h3",
      "lvl4": "article h4",
      "lvl5": "article h5",
      "text": "article p, article li, article pre",
      "lang": {
        "selector": "/html/@lang",
        "type": "xpath",
        "global": true,
        "default_value": "en"
      }
    },
    "language": {
      "lvl0": "article h1",
      "lvl1": "article h2",
      "lvl2": "article h3",
      "lvl3": "article h4",
      "lvl4": "article h5",
      "lvl5": "article td:first-child",
      "text": "article p, article li, article pre, article td:nth-child(2)",
      "lang": {
        "selector": "/html/@lang",
        "type": "xpath",
        "global": true,
        "default_value": "en"
      }
    }
  }
}
