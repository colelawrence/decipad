{
  "editor.tabSize": 2,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "cSpell.userWords": ["decipad"],
  "explorer.compactFolders": false,
  "[typescript]": {
    "editor.codeActionsOnSave": {
      "source.organizeImports": false
    }
  },
  "[typescriptreact]": {
    "editor.codeActionsOnSave": {
      "source.organizeImports": false
    }
  },
  "cSpell.words": [
    "boomify",
    "Buildable",
    "cacheable",
    "camelcase",
    "DECI",
    "docsync",
    "frontends",
    "functionness",
    "GOOGLESHEETS",
    "graphqlserver",
    "NEXTAUTH",
    "nrwl",
    "Topbar",
    "udecode"
  ]
}
